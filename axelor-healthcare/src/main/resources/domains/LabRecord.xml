<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="healthcare" package="com.axelor.apps.healthcare.db"/>

  <entity name="LabRecord">

    <string name="reviewer" title="Reviewer"/>
    <datetime name="dateAndTime" title="date and Time"/>

    <string name="labName" title="Lab Name"/>
    <string name="tests" title="Tests"/>

    <string name="tags" title="Tags"/>

    <many-to-one name="patient" ref="com.axelor.apps.healthcare.db.Patient"
      title="Patient"/>

  </entity>

</domain-models>
