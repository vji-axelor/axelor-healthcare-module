<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="healthcare" package="com.axelor.apps.healthcare.db"/>

  <entity name="PatientRelation">

    <many-to-one ref="com.axelor.apps.healthcare.db.Patient" name="patientReference"
      title="patient Reference"/>
    <string name="relation" title="Relation"
      selection="healthcare-patient-select-patient-relation"/>
    <string name="description" title="Description"/>
    <many-to-one ref="com.axelor.apps.healthcare.db.Patient" name="patient"
      title="patient"/>
  </entity>

</domain-models>
