<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="healthcare" package="com.axelor.apps.healthcare.db"/>

  <entity name="VitalSigns">

    <date name="visitDate" title="Date"/>
    <decimal name="weight" title="Weight Lbs"/>
    <decimal name="height" title="Height Feet"/>
    <string name="bmi" title="BMI"/>
    <integer name="temprature" title="Temprature (F)"/>

    <string name="bpSystolic" title="BP systolic"/>
    <string name="bpDiastolic" title="BP diastolic"/>

    <string name="oxygenSaturation" title="Oxygen Saturation (%)"/>
    <string name="pulseRate" title="Pulse Rate"/>
    <string name="pulsePattern" title="Pulse Pattern"/>
    <string name="pulseVolume" title="pulseVolume"/>

    <many-to-one ref="com.axelor.apps.healthcare.db.PatientEncounter"
      name="patientEncounter" title="Patient Encounter"/>

  </entity>

</domain-models>
