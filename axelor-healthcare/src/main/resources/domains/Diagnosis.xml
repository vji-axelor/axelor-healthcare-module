<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_7.0.xsd">

  <module name="healthcare" package="com.axelor.apps.healthcare.db"/>

  <entity name="Diagnosis">

    <string name="diagnosisType" title="Diagnosis Type"
      selection="healthcare-patient-type-select" default="Veterinary"/>

    <string name="diagnosisName" title="Name" namecolumn="true"/>
    <string name="code" title="Code"/>
    <date name="fromDate" title="From Date"/>
    <date name="toDate" title="To Date"/>
    <string name="diagnosisStatus" title="Diagnosis Status"
      selection="healthcare-diagnosis-status-select"/>

    <string name="notes" title="Notes" large="true"/>


    <many-to-one ref="com.axelor.apps.healthcare.db.Patient" name="patient"
      title="Patient"/>

    <many-to-one ref="com.axelor.apps.healthcare.db.HealthcarePractitioner"
      name="healthcarePractitioner" title="Healthcare Practitioner"/>

    <many-to-one name="companyAssign" ref="com.axelor.apps.base.db.Company"
      title="Associated to"/>

  </entity>

</domain-models>
