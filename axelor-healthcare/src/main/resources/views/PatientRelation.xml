<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="patient-relation-grid" title="Patient Relation"
    model="com.axelor.apps.healthcare.db.PatientRelation">
    <field name="patientReference"/>
    <field name="relation"/>
    <field name="description"/>
  </grid>

  <form name="patient-relation-form" title="Patient Relation"
    model="com.axelor.apps.healthcare.db.PatientRelation" width="large">
    <panel name="mainPanel">
      <field name="patientReference"/>
      <field name="relation"/>
      <field name="description"/>
    </panel>

  </form>
</object-views>
