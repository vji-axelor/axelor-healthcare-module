<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="patient-lab-record-grid" title="Lab Record"
    model="com.axelor.apps.healthcare.db.LabRecord">
    <field name="name"/>
    <field name="reviewer"/>
    <field name="dateAndTime"/>
    <field name="labName"/>
    <field name="tests"/>
    <field name="tags"/>
    <field name="patient"/>
  </grid>

  <form name="patient-lab-record-form" title="Lab Record"
    model="com.axelor.apps.healthcare.db.LabRecord" width="large">
    <panel name="mainPanel">
      <field name="reviewer"/>
      <field name="dateAndTime"/>
      <field name="labName"/>
      <field name="tests"/>
      <field name="tags"/>
    </panel>
  </form>
</object-views>
