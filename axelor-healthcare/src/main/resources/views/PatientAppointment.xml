<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="patient-appointment-grid" title="Patient Appointment"
    model="com.axelor.apps.healthcare.db.PatientAppointment">
    <field name="reference"/>
    <field name="appointmentDate"/>
    <field name="patient"/>
    <field name="healthcarePractitioner"/>
  </grid>

  <form name="patient-appointment-form" title="Patient Appointment"
    model="com.axelor.apps.healthcare.db.PatientAppointment" width="large">
    <panel name="mainPanel">
      <field name="reference"/>
      <field name="appointmentDate"/>
      <field name="patient"/>
      <field name="healthcarePractitioner"/>
    </panel>

  </form>
</object-views>
