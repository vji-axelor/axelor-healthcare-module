<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="patient-address-grid" title="Patient Address"
    model="com.axelor.apps.healthcare.db.PatientAddress">
    <field name="patientAddress"/>
    <field name="isDefaultAddress"/>
  </grid>

  <form name="patient-address-form" title="Patient Address"
    model="com.axelor.apps.healthcare.db.PatientAddress" width="large">
    <panel name="mainPanel">
      <field name="patientAddress" form-view="address-form" grid-view="address-grid"
        canNew="true" canEdit="true"/>
      <field name="isDefaultAddress"/>
    </panel>

  </form>
</object-views>
