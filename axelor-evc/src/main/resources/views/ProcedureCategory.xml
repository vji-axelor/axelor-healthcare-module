<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_7.0.xsd">

  <grid name="evc-procedure-category-grid" title="Procedure Category"
    model="com.axelor.apps.evc.db.ProcedureCategory">
    <field name="name" title="Name"/>
    <field name="code" title="Code"/>
  </grid>

  <form name="evc-procedure-category-form" title="Procedure Category"
    model="com.axelor.apps.evc.db.ProcedureCategory" width="large">
    <panel name="mainPanel">
      <field name="name" title="Name"/>
      <field name="code" title="Code"/>
    </panel>
  </form>
</object-views>
